#include <iostream>
#include "Headers\Calculator.h"

using namespace std;

void Calculator::Addition(double n1, double n2)
{
	cout<<n1 + n2<<endl;
}

void Calculator::Subtraction(double n1, double n2)
{
	cout<<n1 - n2<<endl;
}
